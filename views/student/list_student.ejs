<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<table>
	  <tr>
	  	<th>id</th>
	    <th>First Name</th>
	    <th>Last Name</th>
	    <th>Email</th>
	    <th>Action</th>
	    <th>Options</th>
	  </tr>
	  <%dataStudents.forEach(dataStudent=>{%>
	  <tr>
	  	<td><%=dataStudent.id%></td>
	    <td><%=dataStudent.first_name%></td>
	    <td><%=dataStudent.last_name%></td>
	    <td><%=dataStudent.email%></td>
	    <td> <a href='/students/<%=dataStudent.id%>/addSubject'>add subject</a></td>
	    <td>edit||delete</td>	
	  </tr>
	  <%})%>
	</table>
	
</body>

</html>